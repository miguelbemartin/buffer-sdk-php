{
  "name": "miguelbemartin/buffer-sdk-php",
  "description": "This library allows you to use the Buffer API using PHP",
  "homepage": "https://github.com/miguelbemartin/buffer-sdk-php",
  "license": "MIT",
  "keywords": [
    "Buffer",
    "Bufferapp"
  ],
  "require": {
    "php": ">=7.2",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "symfony/http-client": "^4.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "swaggest/json-diff": "^3.4",
    "phpunit/phpunit": "^8.3"
  },
  "type": "library",
  "autoload": {
    "psr-4": {
      "BufferSDK\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BufferSDK\\Tests\\": "test/"
    }
  }
}
